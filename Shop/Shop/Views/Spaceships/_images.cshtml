@model Shop.Models.Spaceships.ImageViewModel

@{
    var photoPaths = "~/multipleFileUpload/" + (Model.FilePath ?? "noimage.jpg");
}

<form>
    <div class="row">
        <div class="col">
            <div style="display: none;">@Model.FilePath </div>
            <img class="image" src="@photoPaths" asp-append-version="true" />
        </div>
    </div>
</form>
